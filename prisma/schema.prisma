generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING") // Vercel specific
}

model Bolche {
  id          Int      @id @default(autoincrement())
  navn        String
  farve       String
  vaegt       Int
  smagSurhed  String
  smagStyrke  String
  smagType    String
  raavarepris Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("bolcher")
}